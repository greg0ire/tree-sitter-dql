{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "dql",
      "camelcase": "dql",
      "title": "Doctrine Query Language",
      "scope": "source.dql",
      "file-types": [
        "dql"
      ],
      "injection-regex": "^dql$",
      "class-name": "TreeSitterDql",
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm"
    }
  ],
  "metadata": {
    "version": "1.1.0",
    "license": "MIT",
    "description": "Language used by Doctrine ORM to query PHP objects",
    "authors": [
      {
        "name": "Gr√©goire Paris",
        "email": "postmaster@greg0ire.fr",
        "url": "https://github.com/greg0ire"
      }
    ],
    "links": {
      "repository": "https://github.com/greg0ire/dql-treesitter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
